{
  "name": "gapsi-back",
  "version": "1.0.0",
  "description": "Gapsi Back",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon",
    "lint": "eslint src/ --ext .ts",
    "migrate": "npx sequelize-cli db:migrate",
    "prepare": "husky install",
    "seeders": "npx sequelize-cli db:seed:all",
    "serve": "node ./build/server.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "typeScript"
  ],
  "engines": {
    "npm": ">=9.3.1",
    "node": ">=18.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zifris10/gapsi-back.git"
  },
  "author": "Eduardo Mart√≠nez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zifris10/gapsi-back/issues"
  },
  "homepage": "https://github.com/Zifris10/gapsi-back#readme",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.8.2"
